<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build" ToolsVersion="14.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup Condition="'$(Platform)'=='Win32'">
    <IncludePath>$(WindowsSDK_IncludePath);$(WXWIN)\include\msvc;$(WXWIN)\include;$(VC_IncludePath);$(AOCMULTINY_DPLAY_SDK)\Include;$(SolutionDir)\src\;$(SolutionDir\include)</IncludePath>
    <OutDir>$(SolutionDir)$(Configuration)\$(ProjectName)\</OutDir>
    <IntDir>$(Configuration)\$(ProjectName)\</IntDir>
    <LibraryPath>$(VC_LibraryPath_x86);$(WindowsSDK_LibraryPath_x86);$(NETFXKitsDir)Lib\um\x86;$(SolutionDir)\lib</LibraryPath>
    <IncludePath>$(WindowsSDK_IncludePath);$(VC_IncludePath);$(SolutionDir)\include</IncludePath>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Platform)'=='Win32'">
    <ClCompile>
      <ObjectFileName>$(IntDir)\%(RelativeDir)</ObjectFileName>
    </ClCompile>
    <Link>
      <AdditionalLibraryDirectories>$(AOCMULTINY_DPLAY_SDK)\Lib\x86;%(AdditionalLibraryDirectories)</AdditionalLibraryDirectories>
    </Link>
  </PropertyGroup>
</Project>
